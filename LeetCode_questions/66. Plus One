class Solution {
    public int[] plusOne(int[] arr) {
        for(int i=arr.length-1;i>=0;i--){
            if(arr[i]<9){
                arr[i]+=1;
                break;
            }
            else{
                arr[i]=0;
            }
        }
        if(arr[0]==0){
            int[]brr=new int[arr.length+1];
            brr[0]=1;
            return brr;
        }
        return arr;
        }

    }
